nal":{"line":7,"column":7}}],"sources":{"searchParams.js":"import React, { useEffect, useState, useContext } from 'react';\nimport pet, { ANIMALS } from '@frontendmasters/pet';\nimport Results from './Results';\nimport useDropdown from './useDropdown';\nimport ThemeContext from './ThemeContext';\n\nexport const SearchParams = () => {\n    const [location, setLocation] = useState(\"Seattle, WA\");\n    const [breeds, setBreeds] = useState([]);\n    const [animal, AnimalDropdown] = useDropdown(\"Animal\", \"dog\", ANIMALS);\n    const [breed, BreedDropdown, setBreed] = useDropdown(\"Breed\", \"\", breeds);\n    const [pets, setPets] = useState([]);\n    // context using hooks\n    const [theme, setTheme] = useContext(ThemeContext);\n\n    async function requestPets() {\n        const { animals } = await pet.animals({\n            location,\n            breed,\n            type: animal\n         })\n\n         setPets(animals || []);\n    }\n\n    useEffect(() => {\n        setBreeds([]);\n        setBreed(\"\");\n        pet.breeds(animal).then(({ breeds: apiBreeds }) => {\n            const breedStrings = apiBreeds.map(({ name }) => name);\n            setBreeds(breedStrings);\n        }, console.error);\n    }, [animal, setBreed, setBreeds]);\n\n    return (\n        <div className=\"search-params\">\n            <form \n                onSubmit={(e) => {\n                    e.preventDefault();\n                    requestPets();\n            }}\n            > \n                <label htmlFor=\"location\">\n                    location\n                    <input \n                    id=\"location\" \n                    value={location} \n                    placeHolder=\"location\"\n                    onChange={event => setLocation(event.target.value)} />\n                </label>\n                <AnimalDropdown />\n                <BreedDropdown />\n                <label htmlFor=\"theme\">\n                    Theme\n                    <select\n                        value={theme}\n                        onChange={e => setTheme(e.target.value)}\n                        onBlur={e => setTheme(e.target.value)}\n                    >\n                        // the reason we are not using a dropdown here like BreedDropdown above\n                        // is because dropdown creates its own hooks. We are actually using someone\n                        // elses hooks here, we are using apps hooks, not searchParams hooks. Thats\n                        // why we wont use useDropdown here.\n                        <option value=\"peru\">Peru</option>\n                        <option value=\"darkblue\">Dark Blue</option>\n                        <option value=\"mediumorchid\">Medium Orchid</option>\n                        <option value=\"chartreuse\">Chartreuse</option>\n                    </select>\n                </label>\n                <button style={{ backgroundColor: theme }}>Submit</button>\n            </form>\n            <Results pets={ pets }/>\n        </div>\n    );\n};"},"lineCount":null}},"error":null,"hash":"581c913f0c0ff1d8bd4781cdc0b48443","cacheData":{"env":{}}}